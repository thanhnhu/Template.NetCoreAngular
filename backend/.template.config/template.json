{
  "$schema": "http://json.schemastore.org/template",
  "author": "David Pereira",
  "classifications": ["Web", "API", "Microservices"],
  "name": ".NET 6 Template with CQRS, ES and DDD",
  "identity": "Template.NetCore",
  "shortName": "Template.NetCore",
  "tags": {
    "language": "C#"
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**",
            ".git/**",
            "README.md",
            "bin/**",
            "obj/**",
            "images/logo.jpg",
            "images/installation.jpg"
          ]
        }
      ]
    }
  ],
  "sourceName": "Template.NetCore",
  "preferNameDirectory": true,
  "symbols": {
    "author": {
      "type": "parameter",
      "defaultValue": "David Pereira",
      "replaces": "__PROJECT_AUTHOR__"
    },
    "description": {
      "type": "parameter",
      "defaultValue": ".NET 6 Template with CQRS, ES and DDD",
      "replaces": "__PROJECT_DESCRIPTION__"
    },
    "namespace": {
      "type": "parameter",
      "defaultValue": "Template.NetCore",
      "replaces": "Template.NetCore"
    },
    "version": {
      "type": "parameter",
      "defaultValue": "1.0.0",
      "replaces": "99.99.99"
    }
  }
}
